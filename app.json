{
  "expo": {
    "name": "KapKurtar",
    "slug": "kapkurtar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#00A690"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kapkurtar.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00A690"
      },
      "package": "com.kapkurtar.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA"
      ]
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "KapKurtar needs your location to show nearby offers."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1a1b1ddf-0e2c-42ad-914f-8c816ce2b2ed"
      }
    },
    "owner": "kapkurtar"
  }
}